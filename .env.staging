APP_NAME="School Management System (Staging)"
APP_ENV=staging
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=https://staging.your-domain.com

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

BCRYPT_ROUNDS=10

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=database
DB_PORT=3306
DB_DATABASE=school_management_staging
DB_USERNAME=school_user
DB_PASSWORD=

CACHE_STORE=redis
FILESYSTEM_DISK=local
QUEUE_CONNECTION=redis
SESSION_DRIVER=redis
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@staging.example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# Security Settings
SANCTUM_STATEFUL_DOMAINS=localhost,127.0.0.1,staging.your-domain.com
SESSION_SECURE_COOKIE=true
SESSION_SAME_SITE=strict

# Performance Settings
CACHE_PREFIX=school_mgmt_staging
CACHE_TTL=1800

# File Upload Settings
MAX_FILE_SIZE=10240
ALLOWED_FILE_TYPES=pdf,doc,docx,xls,xlsx,ppt,pptx,jpg,jpeg,png,gif

# Analytics Settings
ANALYTICS_ENABLED=true
ANALYTICS_RETENTION_DAYS=90

# Monitoring Settings
MONITORING_ENABLED=true
LOG_RETENTION_DAYS=7